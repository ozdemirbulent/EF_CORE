@model List<string>
@{
    ViewData["Title"] = "PrependMethod";
    Layout = "~/Views/Layout/Index.cshtml";
    int counter = 0;
}


<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h3 class="card-title">prepend İle Mesai Başı Kontrolü</h3>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Açıklama</th>
                            </tr>
                        </thead>
                        <tbody>

                            @foreach (var item in Model)
                            {
                                counter++;
                                <tr>
                                    <th>@counter</th>
                                    <td>@item</td>

                                </tr>
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>



